#include <stdio.h>
#include <stdlib.h>

int main() {
    int v[10];
     int M, m;
  
    for (int i = 0; i < 10; i++) {
        printf("Digite um valor: ");
        scanf("%d", &v[i]);
    }

    M = v[0];
    m = v[0];
    for (int i = 1; i < 10; i++) {
        if (v[i] > M) {
            M = v[i];
        }
        if (v[i] < m) {
            m = v[i];
        }
    }
    printf("\nO maior numero é : %d\n", M);
    printf("O menor numero é  %d\n",m);

    return 0;
}
