#include <stdlib.h>
#include <stdio.h>

int main() {
  int v[8];
  printf("\nDigite numeros:");
  for (int i=0; i<8; i++) {
    scanf("%d", &v[i]);
  }
   int i,j,x,y;
  printf("\nDigite quais as posições que você deseja saber:");
  scanf("%d", &x);
  scanf("%d", &y);  
  i = x;
  j = y;
  
  printf("Os valores das posições são = %d e %d", v[i], v[j]);
  
  return 0;
}
