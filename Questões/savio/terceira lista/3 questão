#include <stdio.h>
#include <stdlib.h>

int main() {
  
  int num[10];
  int maior;
  
  printf("digite um numero: ");
  for(int i = 0 ; i < 10 ; i ++){
  printf("\nnumero %d :", i + 1);
  scanf("%d", &num[i]);  
  }

  maior = num[0];

 for(int i = 1; i < 10 ; i++){
  if(num[i] > maior){
     maior = num[i];   
  }
 }

   printf("o maior valor Ã© : %d ", maior);
   

  
  return 0;
}
