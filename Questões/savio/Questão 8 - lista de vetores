#include <stdio.h>
#include <stdlib.h>
#define T 6

int main() {
  int valores[T]; 
   printf("inicializando...");
  printf("\nesse programa ira escrever 6 valores na ordem contraria");
  //estrutura logica
  for (int i = 0; i < T; i++) {
    printf("\nDigite o valor %d: ", i+1);
    scanf("%d", &valores[i]);
  }
   //resultado 
  printf("\n--Valores  na ordem contraria:\--n");
  for (int i = T-1; i >= 0; i--) {
    printf("%d ", valores[i]); 
  }
  
  return 0;
}
