#include <stdio.h>

int main(void) {
  int v[15], i, m, som;

  printf("Digite as notas dos 15 alunos:\n");

  for (i = 0; i < 15; i++) {
    scanf("%d", &v[i]);
  }

  som = v[0];

  for (i = 1; i < 15; i++) {
    som = som + v[i];
  }

  m = (som) / 15;

  printf("A média geral é = %d", m);
  return 0;
}
