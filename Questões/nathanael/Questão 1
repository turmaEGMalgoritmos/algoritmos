#include <stdio.h>
#include <stdlib.h>

int main(void) {
  int i, v[6]={1, 0, 5, -2, -5, 7}, soma;

  soma=v[0]+v[1]+v[5];

  printf("\nA soma entre os valores das posições A[0], A[1] e A[5] é = %d\n", soma);
  
  v[3] = 100;

  printf("\n---Valores do vetor A---\n");
  
  for (i=0; i<6; i++) {
    printf("A[%d] = %d\n", i, v[i]);
  }
  
  return 0;
}
