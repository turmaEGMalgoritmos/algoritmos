#include <stdio.h>

int main(void) {
int i, a[10], b[10];
  
  printf("\nDigite os valores do vetor A:\n");

  for (i=0; i<10; i++){
    scanf("%d", &a[i]);
    b[i] = a[i]*a[i];
  }

  printf("Os valores do Vetor A é: \n");
  
  for (i=0; i<10; i++){
    printf("A[%d] = %d\n", i, a[i]);
  }

  printf("Os valores do Vetor B é: \n");
  
  for (i=0; i<10; i++){
    printf("B[%d] = %d\n", i, b[i]);
  }

  return 0;
}
